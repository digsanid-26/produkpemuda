<%- contentFor('html_attribute') %>

<%- contentFor('body_attribute') %>

<%- contentFor('extra_css') %>
<style>
  .news-category-badge {
    display: inline-block;
    padding: 4px 12px;
    background: var(--accent-color);
    color: var(--primary-color);
    border-radius: 20px;
    font-size: 12px;
    font-weight: 600;
    margin-bottom: 12px;
  }
  .news-category-badge.event {
    background: var(--primary-color);
    color: var(--white);
  }
  .vl-blog-1-item {
    margin-bottom: 30px;
  }
</style>

<%- contentFor('content') %>
  <%- include('partials/header/navbar5') %>
  
  <!-- ===== HERO AREA STARTS ======= -->
  <div class="inner-pages-section-area" style="background-image: url(/img/all-images/bg/hero-bg1.png); background-position: center; background-repeat: no-repeat; background-size: cover;">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 m-auto">
          <div class="inner-header text-center">
            <h2>News & Event</h2>
            <div class="space24"></div>
            <a href="/">Home <i class="fa-solid fa-angle-right"></i> <span>News & Event</span></a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- ===== HERO AREA ENDS ======= -->

  <!-- ===== NEWS AREA STARTS ======= -->
  <div class="vl-blog-5-area sp2">
    <div class="container">
      <div class="row">
        <% if (typeof articles !== 'undefined' && articles.length > 0) { %>
          <% articles.forEach(article => { %>
        <div class="col-lg-6 col-md-6">
          <div class="vl-blog-1-item">
            <div class="vl-blog-1-thumb image-anime">
              <img src="<%= article.image %>" alt="<%= article.title %>" />
            </div>
            <div class="vl-blog-1-content">
              <span class="news-category-badge<%= article.category === 'Event' ? ' event' : '' %>"><%= article.category %></span>
              <div class="vl-blog-meta">
                <ul>
                  <li>
                    <a href="#">
                      <i class="fa-regular fa-calendar"></i> <%= article.date %> <span>|</span>
                    </a>
                  </li>
                  <li>
                    <a href="#">
                      <i class="fa-regular fa-user"></i> <%= article.author %>
                    </a>
                  </li>
                </ul>
              </div>
              <div class="space24"></div>
              <h4 class="vl-blog-1-title"><a href="/news/<%= article.slug %>"><%= article.title %></a></h4>
              <div class="space16"></div>
              <p><%= article.excerpt %></p>
              <div class="space24"></div>
              <div class="vl-blog-1-icon">
                <a href="/news/<%= article.slug %>">Baca Selengkapnya <i class="fa-solid fa-arrow-right"></i></a>
              </div>
            </div>
          </div>
        </div>
          <% }) %>
        <% } else { %>
        <div class="col-12 text-center py-5">
          <p class="text-muted">Belum ada artikel yang tersedia.</p>
        </div>
        <% } %>
      </div>
    </div>
  </div>
  <!-- ===== NEWS AREA ENDS ======= -->

